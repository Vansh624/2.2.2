MACROS\n\n\n// Robot configuration code.\ninertial BrainInertial = inertial();\n/*vex-vision-config:begin*/\nvision::signature Vision4__C_RED = vision::signature (1, 8085, 9363, 8724,-1297, -133, -716,2.5, 0);\nvision::signature Vision4__C_GREEN = vision::signature (2, -6715, -3931, -5322,-3933, -1739, -2836,2.5, 0);\nvision::signature Vision4__C_BLUE = vision::signature (3, -4421, -3299, -3860,8109, 9893, 9002,2.5, 0);\nvision Vision4 = vision (PORT4, 50, Vision4__C_RED, Vision4__C_GREEN, Vision4__C_BLUE);\n/*vex-vision-config:end*/\n\n\n// generating and setting random seed\nvoid initializeRandomSeed(){\n  wait(100,msec);\n  double xAxis = BrainInertial.acceleration(xaxis) * 1000;\n  double yAxis = BrainInertial.acceleration(yaxis) * 1000;\n  double zAxis = BrainInertial.acceleration(zaxis) * 1000;\n  // Combine these values into a single integer\n  int seed = int(\n    xAxis + yAxis + zAxis\n  );\n  // Set the seed\n  srand(seed);\n}\n\n\n\nvoid vexcodeInit() {\n\n  // Initializing random seed.\n  initializeRandomSeed(); \n}\n\n\n// Generated code.\n\n// Include the EXP Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nint Brain_precision = 0, Console_precision = 0;\n\nfloat myVariable, Color1, Color2, Guess1, Guess2, Control1, Control2, Guess1Correct, Guess2Correct, Choose;\n\nbool Win;\n\nfloat Colors[3];\n\nevent CheckR = event();\nevent message1 = event();\nevent CheckG = event();\nevent CheckB = event();\nevent CheckWin = event();\n\nint mathRandomInt(float a, float b) {\n  if (a > b) {\n    // Swap a and b to ensure a is smaller.\n    float c = a;\n    a = b;\n    b = c;\n  }\n  int tmpA = static_cast<int>(a);\n  int tmpB = static_cast<int>(b);\n  int r = tmpA + rand() / (RAND_MAX / (tmpB - tmpA + 1));\n  return r;\n}\n\n// Used to find the format string for printing numbers with the\n// desired number of decimal places\nconst char* printToBrain_numberFormat() {\n  // look at the current precision setting to find the format string\n  switch(Brain_precision){\n    case 0:  return \"%.0f\"; // 0 decimal places (1)\n    case 1:  return \"%.1f\"; // 1 decimal place  (0.1)\n    case 2:  return \"%.2f\"; // 2 decimal places (0.01)\n    case 3:  return \"%.3f\"; // 3 decimal places (0.001)\n    default: return \"%f\"; // use the print system default for everthing else\n  }\n}\n\n// \"when started\" hat block\nint whenStarted1() {\n  Colors[0] = 1.0;\n  Colors[1] = 2.0;\n  Colors[2] = 3.0;Win = true;\n  Color1 = Colors[static_cast<int>(static_cast<float>(mathRandomInt(1.0, 3.0))) - 1];\n  Choose = static_cast<float>(mathRandomInt(-1.0, 1.0));\n  Guess1 = 4.0;\n  Guess2 = 4.0;\n  if (Choose == 0.0) {\n    Choose = 1.0;\n  }\n  Color2 = Colors[static_cast<int>(Color1 + Choose) - 1];\n  if (Color2 > 3.0) {\n    Color2 = 1.0;\n  } else if (Color2 < 1.0) {\n    Color2 = 3.0;\n  } else {\n  }\n  Control1 = Color1;\n  Control2 = Color2;\n  Brain.Screen.setCursor(4, 1);\n  Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(Color1));\n  Brain.Screen.setCursor(5, 1);\n  Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(Color2));\n  while (Win) {\n    Brain.Screen.clearLine(2);\n    Brain.Screen.setCursor(Brain.Screen.row(), 1);\n    Brain.Screen.setCursor(2, 1);\n    Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(Guess1));\n    Brain.Screen.clearLine(3);\n    Brain.Screen.setCursor(Brain.Screen.row(), 1);\n    Brain.Screen.setCursor(3, 1);\n    Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(Guess2));\n    Brain.Screen.clearLine(4);\n    Brain.Screen.setCursor(Brain.Screen.row(), 1);\n    Brain.Screen.setCursor(4, 1);\n    Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(Color1));\n    Brain.Screen.clearLine(5);\n    Brain.Screen.setCursor(Brain.Screen.row(), 1);\n    Brain.Screen.setCursor(5, 1);\n    Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(Color2));\n    Color1 = Control1;\n    Color2 = Control2;\n    Guess1 = 4.0;\n    Guess2 = 4.0;\n    wait(0.01, seconds);\n    CheckR.broadcast();\n    wait(0.01, seconds);\n    CheckG.broadcast();\n    wait(0.01, seconds);\n    CheckB.broadcast();\n    wait(1.0, seconds);\n    CheckWin.broadcast();\n    wait(0.5, seconds);\n  wait(5, msec);\n  }\n  return 0;\n}\n\n// \"when I receive CheckR\" hat block\nvoid onevent_CheckR_0() {\n  Vision4.takeSnapshot(Vision4__C_RED);\n  if (Vision4.objectCount > 0) {\n    if (!(Guess1 == 4.0)) {\n      Guess2 = 1.0;\n      Brain.Screen.clearLine(3);\n      Brain.Screen.setCursor(Brain.Screen.row(), 1);\n      Brain.Screen.setCursor(3, 1);\n      Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(Guess2));\n    }\n    else {\n      Guess1 = 1.0;\n      Brain.Screen.clearLine(2);\n      Brain.Screen.setCursor(Brain.Screen.row(), 1);\n      Brain.Screen.setCursor(2, 1);\n      Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(Guess1));\n    }\n  }\n  else {\n  }\n}\n\n// \"when I receive CheckG\" hat block\nvoid onevent_CheckG_0() {\n  Vision4.takeSnapshot(Vision4__C_GREEN);\n  if (Vision4.objectCount > 0) {\n    if (!(Guess1 == 4.0)) {\n      Guess2 = 2.0;\n      Brain.Screen.clearLine(3);\n      Brain.Screen.setCursor(Brain.Screen.row(), 1);\n      Brain.Screen.setCursor(3, 1);\n      Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(Guess2));\n    }\n    else {\n      Guess1 = 2.0;\n      Brain.Screen.clearLine(2);\n      Brain.Screen.setCursor(Brain.Screen.row(), 1);\n      Brain.Screen.setCursor(2, 1);\n      Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(Guess1));\n    }\n  }\n  else {\n  }\n}\n\n// \"when I receive CheckB\" hat block\nvoid onevent_CheckB_0() {\n  Vision4.takeSnapshot(Vision4__C_BLUE);\n  if (Vision4.objectCount > 0) {\n    if (!(Guess1 == 4.0)) {\n      Guess2 = 3.0;\n      Brain.Screen.clearLine(3);\n      Brain.Screen.setCursor(Brain.Screen.row(), 1);\n      Brain.Screen.setCursor(3, 1);\n      Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(Guess2));\n    }\n    else {\n      Guess1 = 3.0;\n      Brain.Screen.clearLine(2);\n      Brain.Screen.setCursor(Brain.Screen.row(), 1);\n      Brain.Screen.setCursor(2, 1);\n      Brain.Screen.print(printToBrain_numberFormat(), static_cast<float>(Guess1));\n    }\n  }\n  else {\n  }\n}\n\n// \"when I receive CheckWin\" hat block\nvoid onevent_CheckWin_0() {\n  Brain.Screen.clearLine(1);\n  Brain.Screen.setCursor(Brain.Screen.row(), 1);\n  Brain.Screen.setCursor(1, 1);\n  if (Guess1 == Color1) {\n    Brain.Screen.print(\"One guess is correct\");\n    Guess1Correct = 1.0;\n    Color1 = 0.0;\n  } else if (Guess1 == Color2) {\n    Brain.Screen.print(\"One guess is correct\");\n    Guess1Correct = 1.0;\n    Color2 = 0.0;\n  } else {\n  }\n  if (Guess2 == Color1) {\n    Brain.Screen.print(\"One guess is correct\");\n    Guess2Correct = 1.0;\n    Color1 = 0.0;\n  } else if (Guess2 == Color2) {\n    Brain.Screen.print(\"One guess is correct\");\n    Guess2Correct = 1.0;\n    Color2 = 0.0;\n  } else {\n  }\n  if (Guess1Correct == 1.0 && Guess2Correct == 1.0) {\n    wait(1.0, seconds);\n    Win = false;\n    Brain.Screen.clearScreen();\n    Brain.Screen.setCursor(1, 1);\n    Brain.Screen.print(\"YOU WIN\");\n  }\n}\n\n\nint main() {\n  // Initializing Robot Configuration. DO NOT REMOVE!\n  vexcodeInit();\n\n  // register event handlers\n  CheckR(onevent_CheckR_0);\n  CheckG(onevent_CheckG_0);\n  CheckB(onevent_CheckB_0);\n  CheckWin(onevent_CheckWin_0);\n\n  wait(15, msec);\n  // post event registration\n\n  // set default print color to black\n  printf(\"\\033[30m\");\n\n  // Wait for ADI sensors to initialize\n  wait(100, msec);\n\n\n  whenStarted1();\n}","target":"Physical"}